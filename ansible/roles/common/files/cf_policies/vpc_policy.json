{
  "Statement" : [
    {
      "Effect" : "Deny",
      "Principal" : "*",
      "Action" : ["Update:Replace", "Update:Delete"],
      "Resource" : "*",
      "Condition" : {
        "StringEquals" : {
          "ResourceType" : ["AWS::EC2::NatGateway", "AWS::EC2::EIP"]
        }
      }
    },
    {
      "Effect" : "Allow",
      "Action" : "Update:*",
      "Principal": "*",
      "Resource" : "*"
    }
  ]
}