<source>
  @type tail
  format /^(?<time>(?:\S+\s+){2}\S+) (?<host>[^ ]+) (?<process>[^ ]+) (?<module>\[.*?\]) (?<thread>\[.*?\]) (?<level>[^ ]+) (?<message>.*)/
  time_format %b %d %H:%M:%S
  path /var/log/xgemail/jilter.log
  tag sophos.xgemail.<%= @application_name %>.jilter
  pos_file /var/log/td-agent/jilter.pos
</source>
