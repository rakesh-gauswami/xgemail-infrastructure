<source>
  @type tail
  format /^(?<time>(?:\S+\s+){2}\S+) (?<host>[^ ]+) (?<process>[^ ]+) (?<pid>[^ ]+) (?<level>[^ ]+) (?<message>.*)/
  time_format %b %d %H:%M:%S
  path /var/log/xgemail/lifecycle.log
  tag sophos.xgemail.<%= @application_name %>.lifecycle
  pos_file /var/log/td-agent/lifecycle.pos
</source>
