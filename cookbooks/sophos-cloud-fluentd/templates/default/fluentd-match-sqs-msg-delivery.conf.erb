<match sqs.msg.delivery.succeeded>
  @type sqs
  queue_name <%= @delivery_status_queue %>
  create_queue false
  region <%= @region %>
  delay_seconds <%= @sqs_delivery_delay %>
  include_tag false
</match>
<match sqs.msg.delivery.failed>
