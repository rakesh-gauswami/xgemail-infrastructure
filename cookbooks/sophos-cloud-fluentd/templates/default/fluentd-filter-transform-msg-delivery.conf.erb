<filter sns.msg.delivery.**>
  @type record_transformer
  <record>
    account <%= @account %>
    region <%= @region %>
    application_name xgemail.<%= @application_name %>
    server_type <%= @server_type %>
    server_ip <%= @server_ip %>
    direction <%= @direction %>
    instance_id <%= @instance_id %>
    timestamp ${time.strftime("%Y-%m-%dT%H:%M:%S%z")}
  </record>
</filter>