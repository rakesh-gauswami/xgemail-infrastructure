<filter sns.msg.delivery.**>
  @type record_transformer
  enable_ruby
  <record>
    server_type <%= @server_type %>
    server_ip <%= @server_ip %>
    server_type_xdelivery <%= @server_type_xdelivery %>
    direction <%= @direction %>
    timestamp ${time.strftime("%Y-%m-%dT%H:%M:%S%z")}
  </record>
</filter>

<filter sqs.msg.returned>
  @type record_transformer
  enable_ruby
  <record>
    server_type <%= @server_type %>
    server_ip <%= @server_ip %>
    server_type_xdelivery <%= @server_type_xdelivery %>
    direction <%= @direction %>
    timestamp ${time.strftime("%Y-%m-%dT%H:%M:%S%z")}
    dsn_code <%= @non_delivery_dsn %>
    recipient_address null
  </record>
</filter>

<filter sqs.msg.delivery.**>
