<filter sns.msg.delivery.**>
  @type record_transformer
  enable_ruby
  <record>
    server_type <%= @server_type %>
    server_ip <%= @server_ip %>
    server_type_xdelivery <%= @server_type_xdelivery %>
    server_ip_xdelivery <%= @server_ip_xdelivery %>
    direction <%= @direction %>
    timestamp ${time.strftime("%Y-%m-%dT%H:%M:%S%z")}
  </record>
</filter>