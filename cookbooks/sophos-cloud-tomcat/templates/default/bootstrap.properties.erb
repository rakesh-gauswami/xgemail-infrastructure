elasticSearch.clusterName = elasticsearch_cluster
elasticSearch.addresses = <%= @elasticsearch_addresses %>
elasticSearch.recordBufferSize = 512
elasticSearch.pollingMs = 60000

redisPool.addresses = <%= @redis_addresses %>
redisPool.useSSL = false
redisPool.password = <%= @redis_auth %>
redisPool.db = <%= @redis_database %>
redisPool.db.[redisUpe] = 0
redisPool.db.[redisMcs] = 1
redisPool.db.[redisMobile] = 2
redisPool.useSentinel = false
redisPool.masterName =
redisPool.minIdle = 5
redisPool.maxIdle = 25
redisPool.maxTotal = 50
redisPool.heartbeat = true
redisPool.heartbeatInterval = 5000

mongoClient.addresses = <%= @mongodb_addresses %>
mongoClient.source = admin
mongoClient.useSSL = true
mongoClient.username = <%= @mongodb_username %>
mongoClient.password = <%= @mongodb_password %>
mongoContext.database.[mongoContext] = <%= @mongodb_database %>
mongoContext.database.[mongoContextMcs] = <%= @mongodb_ep_database %>

clusterServices.clusterName = Sophos
clusterServices.basePort = 7800
clusterServices.portRange = 10

zero.url = <%= @zero_url %>
zero.authId = <%= @zero_id %>
zero.authToken = <%= @zero_token %>

iapi.authId = <%= @upe_id %>
iapi.authToken = <%= @upe_token %>

deployment.apiUrl = https://@@SVC_API@@
deployment.mcsUrl = https://@@SVC_MCS@@/sophos
deployment.coreUrl = https://@@SVC_CORE@@
deployment.mobileUrl = https://@@SVC_MOB@@
deployment.smcUrl = https://@@SVC_SMC@@
deployment.zeroUrl = https://@@IAPI_P0@@/product

system.jmxUser = sophos
system.jmxPassword = admin

mobile.baseUrl = @@MOBILE_FRONTEND@@
